{
  "globalStyle": {
    "navigationStyle": "default",
    "navigationBarTitleText": "钦州海风",
    "navigationBarBackgroundColor": "#0A57ED",
    "navigationBarTextStyle": "black",
    "backgroundColor": "#FFFFFF"
  },
  "easycom": {
    "autoscan": true,
    "custom": {
      "^uni-(.*)": "@dcloudio/uni-ui/lib/uni-$1/uni-$1.vue",
      "^uv-(.*)": "@climblee/uv-ui/components/uv-$1/uv-$1.vue",
      "^(?!z-paging-refresh|z-paging-load-more)z-paging(.*)": "z-paging/components/z-paging$1/z-paging$1.vue"
    }
  },
  "tabBar": {
    "color": "#999999",
    "selectedColor": "#3254FF",
    "backgroundColor": "#F8F8F8",
    "borderStyle": "black",
    "height": "50px",
    "fontSize": "10px",
    "iconWidth": "24px",
    "spacing": "3px",
    "list": [
      {
        "iconPath": "static/tabbar/home.png",
        "selectedIconPath": "static/tabbar/homeHL.png",
        "pagePath": "pages/home/index",
        "text": "概览"
      },
      {
        "iconPath": "static/tabbar/mine.png",
        "selectedIconPath": "static/tabbar/mineHL.png",
        "pagePath": "pages/monitor/index",
        "text": "监控"
      }
    ]
  },
  "pages": [
    {
      "path": "pages/home/index",
      "type": "home",
      "style": {
        "navigationBarTitleText": "工程概览"
      },
      "needLogin": true
    },
    {
      "path": "pages/login/index",
      "type": "page",
      "style": {
        "navigationStyle": "custom"
      }
    },
    {
      "path": "pages/monitor/detail",
      "type": "page",
      "style": {
        "navigationBarTitleText": "视频详情"
      },
      "needLogin": true
    },
    {
      "path": "pages/monitor/index",
      "type": "page",
      "style": {
        "navigationBarTitleText": "视频监控"
      },
      "needLogin": true
    },
    {
      "path": "uni_modules/uni-upgrade-center-app/pages/upgrade-popup",
      "style": {
        "disableScroll": true,
        "app-plus": {
          "backgroundColorTop": "transparent",
          "background": "#00000000",
          "titleNView": false,
          "scrollIndicator": "none",
          "popGesture": "none",
          "animationType": "fade-in",
          "animationDuration": 200
        }
      }
    }
  ],
  "subPackages": []
}